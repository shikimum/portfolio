<% @mangas.each do |manga| %>
  <li class="w-80 flex items-center gap-x-3.5 py-2 px-3 rounded-md text-sm text-dark hover:bg-blue-200 focus:ring-2 focus:ring-blue-500 dark:text-gray-400 dark:hover:bg-blue-200 dark:hover:text-gray-300" role="option" data-autocomplete-value="<%= manga.id %>" data-autocomplete-label="<%= manga.title.gsub(/[\r\n]/, ' ') %>">
    <%= manga.title %>
  </li>
<% end %>